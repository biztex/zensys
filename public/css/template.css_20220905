.container-plan div.price2 {
    text-align: center;
    font-weight: bold;
    margin-top: 30px;
    margin-bottom: 15px;
}

.container-plan div.price2 .type {
    margin-bottom: 0;
}

.calendar .schedule td .price_detail1,
.calendar .schedule td .price_detail2 {
    display: block;
    text-align: left;
    padding: 0px;
    font-weight: bold;
    border: none;
    font-size: 12px !important;
    margin-bottom: 10px;
    /* 文字の折り返し */
    word-break: break-all;
}

.calendar .schedule td .price_detail1:nth-last-of-type(1),
.calendar .schedule td .price_detail2:nth-last-of-type(1) {
    margin-bottom: 0px;
}

.calendar .schedule td .selected-date {
    display: block;
    margin-bottom: 10px;
}

@media screen and (max-width: 767px) {

    .calendar .schedule td .price_detail1,
    .calendar .schedule td .price_detail2 {
        font-size: 10px !important;
        margin: 0;
        padding: 0 5px;
    }
}

